

create database Hostel_management_system;
create table Student(
studentID varchar(20) NOT NULL,                       
student_name varchar(20) NOT NULL,
course_ID varchar(20) NOT NULL,
roomID varchar(20) NOT NULL,
phone_number varchar(20) NOT NULL,
term varchar(20) NOT NULL,
date_of_birth date NOT NULL,
hostel_name varchar(20) NOT NULL,
primary key (studentID));


create table hostel(


hostel_name varchar(20) NOT NULL,                    
num_of_rooms int NOT NULL,
num_of_beds int  NOT NULL,
num_of_Students int  NOT NULL,
incharge_name varchar(20) NOT NULL,

primary key (hostel_name));



create table room(                                    


roomID varchar(20) NOT NULL,

num_of_beds int NOT NULL,
num_of_Students int NOT NULL,
hostel_name varchar(20) NOT NULL,

primary key (roomID));




create table payment(                                 
paymentID varchar(20) NOT NULL,
studentID varchar(20) NOT NULL,

Due_Date date NOT NULL,

method_of_payment varchar(20) NOT NULL,
hostel_fee varchar(20) NOT NULL,
late_fees varchar(20) NOT NULL,
primary key(paymentID))




create table mess(                                      
messID varchar(20) NOT NULL,
meal_type varchar(20) NOT NULL,
time_ varchar(20) NOT NULL,
capacity int  NOT NULL,
hostel_name varchar(20) NOT NULL)


create table visitors(                                  
guardian_name varchar(20) NOT NULL,
date_of_visit date NOT NULL,
studentID varchar(20) NOT NULL,
phone_number varchar(20) NOT NULL,
incharge_name varchar(20) NOT NULL)


