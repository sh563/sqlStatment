sql statements for requirements:

1)
*information to student with studentId
SELECT hostel_name,student_name,course_ID,roomID,phone_number,term,date_of_birth
FROM Student
WHERE studentID = ANY (SELECT studentID FROM Student WHERE studentID = 'c0757705');

*information to students regarding their mess schedule with hostel name

select time_,messID,hostel_name,meal_type
from mess
WHERE hostel_name = ANY (SELECT hostel_name FROM mess WHERE hostel_name = 'hostel_A');
*payment information to student
SELECT paymentID,studentID,hostel_fee,late_fees,Due_Date
FROM payment
WHERE studentID = ANY (SELECT studentID FROM Student WHERE studentID = 'c0757705');

2) payment report for management with student information 
SELECT paymentID,studentID,hostel_fee,late_fees
FROM payment
WHERE Due_Date = ANY (SELECT Due_Date FROM payment WHERE Due_Date = '');


*for management to check who visited hostel on which date 
SELECT guardian_name,studentID,phone_number,incharge_name
FROM visitors
WHERE date_of_visit = ANY (SELECT date_of_visit FROM visitors WHERE date_of_visit = '2019-08-12');
*according to incharge name
SELECT guardian_name,studentID,phone_number,incharge_name
FROM visitors
WHERE incharge_name = ANY (SELECT incharge_name FROM visitors WHERE incharge_name = 'Nitika');
 
3)
*space according to hostel:
select hostel.hostel_name, hostel.num_of_beds,hostel.num_of_Students,(num_of_beds-num_of_Students) as space 
from hostel;


*space according to rooms
select room.roomID,room.num_of_beds,room.num_of_Students,(num_of_beds-num_of_Students) as space_available
from room;

4)
 *for mess manager to get information about how many students will eat at what time 
SELECT mess.messID,mess.capacity,mess.meal_type,mess.time_, hostel.num_of_Students
FROM hostel,mess

where hostel.hostel_name = mess.hostel_name;

*for checking availabity in mess
select mess.capacity,hostel.num_of_Students,( mess.capacity-hostel.num_of_Students) as available_space
FROM hostel,mess

where hostel.hostel_name = mess.hostel_name;

	


